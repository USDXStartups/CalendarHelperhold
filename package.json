{
  "name": "stationfy-web",
  "version": "0.0.1",
  "description": "Stationfy Web App.",
  "main": "index.js",
  "engines": {
    "node": "5.9.1"
  },
  "scripts": {
    "test": "jest",
    "vendors": "cp -r app/vendors public/",
    "build:css": "node-sass -r app/sass/pages/ -o public/css/",
    "watch:css": "npm run build:css -- -w",
    "minify": "npm run minify-imgs && npm run minify-html",
    "minify-imgs": "imagemin app/imgs/** --out-dir=public/imgs",
    "minify-html": "html-minifier app/index.html --html5 --collapse-whitespace -o public/index.html",
    "favicon": "cp app/favicon.ico public/favicon.ico",
    "build:dev": "rm -rf public && npm run minify && webpack && npm run favicon && npm run vendors && npm run build:css",
    "build": "rm -rf public && npm run minify && webpack && npm run favicon && npm run vendors && npm run build:css && node hashmark.js",
    "watch": "webpack --watch",
    "start": "node server.js",
    "server": "babel-node app.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stationfy/web.git"
  },
  "keywords": [
    "stationfy",
    "video",
    "sports"
  ],
